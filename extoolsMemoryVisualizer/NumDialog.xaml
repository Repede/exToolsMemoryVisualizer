<Window x:Class="extoolsMemoryVisualizer.NumDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:extoolsMemoryVisualizer"
        mc:Ignorable="d"
        Title="NumDialog" Height="190" Width="300">
	<Grid>
		<!-- Accept or Cancel -->
		<StackPanel Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="4">
			<Label Grid.Column="1" Grid.Row="0">Sensitivity</Label>
			<TextBox Name="sensitivity" Grid.Column="1" Grid.Row="0">
			</TextBox>
			<Button Name="okButton" Click="okButton_Click" IsDefault="True">OK</Button>
			<Button Name="cancelButton" IsCancel="True">Cancel</Button>
		</StackPanel>
	</Grid >
</Window>
